    "manifest_version": 2,
    "name": "Google Review Extension",
    "version": "1.0",
    "permissions": [
        "activeTab"
    ],
    "background": {
        "scripts": ["../js/background.js"]
    },
    "content_scripts": [
        {
            "matches": ["*://*.deliveroo.com/*"],
            "js": ["../js/content.js"]
        }
    ]
}
